<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="2.0"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:noNamespaceSchemaLocation=
                   "http://ant.apache.org/ivy/schemas/ivy.xsd">
	<info organisation="schemacrawler" module="schemacrawler-spring" />
	<dependencies>
		<dependency org="org.apache.velocity" name="velocity" rev="1.5" />
		<dependency org="org.freemarker" name="freemarker" rev="2.3.16" />
		<dependency org="org.springframework" name="spring-context" rev="2.5.6" />
		<dependency org="org.springframework" name="spring-jdbc" rev="2.5.6" />

		<exclude org="javax.ejb"/>
		<exclude org="javax.jms"/>
		<exclude org="javax.transaction"/>
		<exclude org="javax.resource"/>
		<exclude org="com.bea.wlplatform"/>
		<exclude org="jotm"/>
		<exclude org="ant"/>
		
        <exclude type="sources" ext="jar"/>
        <exclude type="test" ext="jar"/>
        <exclude type="javadoc" ext="jar"/>
	</dependencies>
</ivy-module>
